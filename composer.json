{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": "^7.1.3",
        "ext-json": "*",
        "bjsmasth/php-salesforce-rest-api": "^0.2.0",
        "fideloper/proxy": "^4.0",
        "flow/jsonpath": "0.3.1",
        "fzaninotto/faker": "^1.8",
        "laravel/framework": "5.7.*",
        "laravel/tinker": "^1.0",
        "league/csv": "^9.1",
        "marquine/php-etl": "2.2",
        "microsoft/microsoft-graph": "^1.10",
        "optimus/bruno": "~3.0",
        "prettus/laravel-request-logger": "^1.2",
        "squizlabs/php_codesniffer": "*",
        "tmilos/scim-filter-parser": "^1.3",
        "adldap2/adldap2-laravel": "^6.0",
        "yajra/laravel-oci8": "5.7.*"
    },
    "require-dev": {
        "beyondcode/laravel-dump-server": "^1.0",
        "filp/whoops": "^2.0",
        "mockery/mockery": "^1.0",
        "nunomaduro/collision": "^2.0",
        "phpunit/phpunit": "^7"
    },
    "autoload": {
        "classmap": [
            "database/seeds",
            "database/factories",
            "app/"
        ],
        "psr-4": {
            "App\\": "app/"
        },
        "files": [
            "app/Http/Helper/functions.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": [
            ]
        }
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
